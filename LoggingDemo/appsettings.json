{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
      "FormatterName": "CustomColorFormatter",
      "FormatterOptions": {
        "CustomPrefix": "|-<[",
        "CustomSuffix": "]>-|",
        "SingleLine": true,
        "IncludeScopes": false,
        "TimestampFormat": "HH:mm:ss.ffff ",
        "UseUtcTimestamp": true,
        "JsonWriterOptions": {
          "Indented": true
        }
      }
  },
  "AllowedHosts": "*"
}
